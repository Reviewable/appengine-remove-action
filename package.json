{
  "name": "appengine-remove",
  "version": "0.1.0",
  "description": "Github Action: Remove versions from Google App Engine",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && ncc build --source-map --no-source-map-register src/main.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write src/*.ts"
  },
  "keywords": [
    "actions",
    "google cloud",
    "app engine",
    "deploy"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.3",
    "@actions/tool-cache": "^2.0.1",
    "@google-github-actions/setup-cloud-sdk": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vercel/ncc": "^0.36.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
